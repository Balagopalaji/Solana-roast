NODE_ENV=development
PORT=3000
CORS_ORIGIN=http://localhost:5173
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Required API Keys
OPENAI_API_KEY=your-openai-api-key
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_NETWORK=devnet  # or mainnet-beta if you prefer 

# Optional API Keys
SOLSCAN_API_KEY=your-solscan-api-key

# Imgflip API credentials
IMGFLIP_USERNAME=your-imgflip-username
IMGFLIP_PASSWORD=your-imgflip-password


# Frontend URL for share links
FRONTEND_URL=http://localhost:5173

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_SOLANA_RPC_URL=https://api.devnet.solana.com
VITE_SOLANA_NETWORK=devnet

# Feature Flags
VITE_ENABLE_SOCIAL_FEATURES=true 

# Alchemy API Key
ALCHEMY_API_KEY=your-alchemy-api-key

# Twitter API Keys
TWITTER_API_KEY=your-twitter-api-key
TWITTER_API_SECRET=your-twitter-api-secret
TWITTER_ACCESS_TOKEN=your-twitter-access-token
TWITTER_ACCESS_SECRET=your-twitter-access-secret

# Twitter OAuth 2.0
TWITTER_CLIENT_ID=your-twitter-client-id
TWITTER_CLIENT_SECRET=your-twitter-client-secret
TWITTER_TOKEN_ENCRYPTION_KEY=your-32-byte-encryption-key

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
VITE_CLOUDINARY_UPLOAD_PRESET=your-cloudinary-upload-preset
VITE_ENABLE_TWITTER=true

# Security
ENCRYPTION_KEY=your-encryption-key

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_MAX_RETRIES=3
REDIS_HEALTH_CHECK_INTERVAL=30000
REDIS_CONNECTION_TIMEOUT=5000
REDIS_MAX_POOL_SIZE=20

# For production Redis (uncomment and configure)
# REDIS_TLS=true
# REDIS_URL=rediss://your-production-redis-url
# REDIS_PASSWORD=your-production-redis-password
# REDIS_MAX_RETRIES=5
# REDIS_HEALTH_CHECK_INTERVAL=15000
# REDIS_CONNECTION_TIMEOUT=3000
# REDIS_MAX_POOL_SIZE=50

# Redis Monitoring (uncomment and configure for production)
# REDIS_MONITOR_ENABLED=true
# REDIS_MONITOR_INTERVAL=60000
# REDIS_ALERT_THRESHOLD_MEMORY=80
# REDIS_ALERT_THRESHOLD_LATENCY=100
# REDIS_ALERT_THRESHOLD_ERRORS=10